{
    "builds": [{
        "src": "ToDoApp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    },
    {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config" :{
            "disDir":"staticfiles_build"
        }
    }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest" : "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ToDoApp/wsgi.py"
        }
    ]
}